<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd"
  default-init-method="init" default-destroy-method="destroy">

  <bean class="com.memorynotfound.spring.core.lifecycle.Course">
    <property name="name" value="course default init and destroy methods"/>
  </bean>

  <bean class="com.memorynotfound.spring.core.lifecycle.Student">
    <property name="name" value="student default init and destroy methods"/>
  </bean>

</beans>
